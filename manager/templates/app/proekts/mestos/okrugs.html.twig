{% extends 'probase.html.twig' %}

{% block title %}Округа{% endblock %}

{% block menupro %}
    {{ include('app/proekts/_tabs.html.twig') }}
{% endblock %}

{% block kroxii %}
            <a href="{{ path('app.proekts.basepro') }}">Главная</a>
{#            <a href="{{ path('proekts.mestos.inform') }}">НомерМеста</a>#}
            <span class="aktiv ">Номера Округов</span> 
{% endblock %}


{% block bodyy %}

{% block sidebar %}
    {{ include('app/proekts/mestos/side/_sideokrug.html.twig') }}
{% endblock %}

<div  class="proekts">

    <div class="wstavka">
        <div class="wstavka__title nav-teni">
            Место расположения пасеки: Округ
        </div>
    </div>

        <h2 class = "pagee__title ">Выбираем свой Округ </h2>

            {% for okrug in okrugs %}
                <div class="mestoo ">
                    <div class="mestoo__raspol">
                        <ul class="mestoo__title">
                            <li class="mestoo__title-item mestoo__nomer-red">
                                <a href="{{ path('app.proekts.mestos.oblasts', {'okrug_id': okrug.id}) }}">{{ okrug.nomer }} </a>
                            </li>
                            <li class="mestoo__title-item ">
                                <a href="{{ path('app.proekts.mestos.oblasts', {'okrug_id': okrug.id}) }}">
                                    {{ okrug.name }}  ( {{ okrug.oblasts }}  )
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            {% endfor %}
        {# </div> #}
      
</div>

{% endblock %}